{
  "name": "AppDayNative",
  "version": "0.0.1",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build:android": "react-native bundle --platform android --dev false --entry-file src/index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "build:apk-release": "cd android && ./gradlew clean && ./gradlew assembleRelease",
    "build:apk-debug": "cd android && ./gradlew clean && ./gradlew assembleDebug",
    "web": "react-scripts start",
    "web:build": "react-scripts build",
    "android": "react-native run-android --variant=release",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --quiet",
    "lint-fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix --fix-type suggestion,layout"
  },
  "dependencies": {
    "@popperjs/core": "^2.6.0",
    "bootstrap": "^4.6.0",
    "jquery": "^3.5.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-native": "0.63.4",
    "react-native-web": "^0.14.10",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "tslib": "^2.0.3"
  },
  "homepage": "http://localhost:3000/",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-syntax-class-properties": "^7.12.1",
    "@babel/plugin-syntax-json-strings": "^7.8.3",
    "@babel/plugin-syntax-jsx": "^7.12.1",
    "@babel/plugin-syntax-typescript": "^7.12.1",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/plugin-transform-react-jsx": "^7.12.12",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-flow": "^7.12.1",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/cli": "^4.13.1",
    "@react-native-community/eslint-config": "^2.0.0",
    "@react-navigation/core": "^5.15.1",
    "@react-navigation/native": "^5.9.2",
    "@react-navigation/routers": "^5.7.1",
    "@react-navigation/stack": "^5.14.1",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-native": "^0.63.46",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "babel-jest": "^26.6.0",
    "babel-loader": "8.1.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-spellcheck": "^0.0.17",
    "fork-ts-checker-webpack-plugin": "^6.0.8",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.6",
    "jest": "26.6.0",
    "jest-react-native": "^18.0.0",
    "metro-react-native-babel-preset": "^0.64.0",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "pretty-quick": "^3.1.0",
    "query-string": "^6.13.8",
    "react-native-dotenv": "^2.5.0",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-safe-area-view": "^1.1.1",
    "react-navigation": "^4.4.3",
    "react-scripts": "^4.0.1",
    "react-test-renderer": "16.13.1",
    "sanitize-html": "^2.3.0",
    "split-on-first": "^2.0.1",
    "strict-uri-encode": "^2.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "tslint-etc": "^1.13.9",
    "tslint-react": "^5.0.0",
    "tslint-react-hooks": "^2.2.2",
    "tsutils": "^3.17.1",
    "typescript": "^4.1.3",
    "webpack": "4.44.2",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "3.11.0"
  },
  "assetBundlePatterns": [
    "src/assets/**/*"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
