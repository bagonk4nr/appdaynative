{
  "compilerOptions": {
    "outDir": "./dist",
    "allowJs": false,
    "target": "es2020",
    "sourceMap": true,
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "module": "esnext",
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "es2018",
      "es2017",
      "es2016",
      "es2015",
      "es6",
      "es5"
    ],
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "assetBundlePatterns": [
    "assets"
  ],
  "assets": [
    "assets"
  ],
  "include": [
    "src",
    "assets"
  ],
  "exclude": [
    "node_modules"
  ]
}
